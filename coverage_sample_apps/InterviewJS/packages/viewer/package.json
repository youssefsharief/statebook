    "Piotr Fedorczyk <piotr@piotrf.pl>"
  ],
  "homepage": "https://story.interviewjs.io/",
  "dependencies": {
    "axios": "^0.18.0",
    "clone": "^1.0.4",
    "date-fns": "^1.29.0",
    "interviewjs-styleguide": "*",
    "lodash": "^4.17.5",
    "prop-types": "15.6.1",
    "raven-for-redux": "^1.3.0",
    "raven-js": "^3.24.0",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-ga": "^2.4.1",
    "react-modal": "3.3.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-redux": "4.0.8",
    "react-scripts": "1.1.1",
    "react-share": "^2.1.0",
    "recompose": "0.26.0",
    "redux": "3.7.2",
    "redux-actions": "2.3.0",
    "redux-devtools-extension": "2.13.2",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "uuid": "3.2.1",
    "styled-components": "3.3.3"
  },
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.7.0",
    "git-revision-webpack-plugin": "^2.5.1",
    "husky": "0.14.3",
    "lint-staged": "7.0.4",
    "node-plop": "0.13.0",
    "plop": "2.0.0",
    "prettier": "1.14.2",
    "react-app-rewire-babel-loader": "0.1.1",
    "react-app-rewire-define-plugin": "^1.0.0",
    "react-app-rewired": "1.5.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build && cd ./build && zip -9 -r ./archive.zip * -x '*.DS_Store'",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "prettier": "prettier --print-width=120 --trailing-comma es5",
    "format": "npm run prettier -- --write '{,!(build|generators)/**/}*.js'",
    "precommit": "lint-staged",
    "eslint-check":
      "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint **/*.js --quiet",
    "lint-fix": "eslint --fix",
    "generate": "plop --plopfile generators/index.js"
  },
  "lint-staged": {
    "{,!(build|generators)/**/}*.js": ["npm run prettier -- --write", "git add"]
  },
  "engines": {
    "npm": ">=4",
    "node": ">=6"
  }
}
